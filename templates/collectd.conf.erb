# Generated by Puppet

<% unless scope.lookupvar('collectd::fqdnlookup') %>
Hostname <%= scope.lookupvar('::hostname') %>
FQDNLookup false
<% else %>
#Hostname localhost
FQDNLookup true
<% end %>
#BaseDir "/var/lib/collectd"
#PluginDir "/usr/lib/collectd"
#TypesDB "/usr/share/collectd/types.db" "/etc/collectd/my_types.db"
Interval <%= scope.lookupvar('collectd::interval') %>
Timeout <%= scope.lookupvar('collectd::timeout') %>
ReadThreads <%= scope.lookupvar('collectd::threads') %>
Include "<%= plugin_conf_dir %>"
